<%= form_with(model: division, local: true) do |form| %>
  <% if division.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(division.errors.count, "error") %> prohibited this division from being saved:</h2>

      <ul>
      <% division.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :division %>
    <%= form.text_field :division %>
  </div>

  <div class="field">
    <%= form.label :quantiy %>
    <%= form.text_field :quantiy %>
  </div>

  <div class="field">
    <%= form.label :standard_id %>
     <%=form.collection_select(:standard_id , Standard.all, :id,:name, :include_blank => "select teacher") %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
